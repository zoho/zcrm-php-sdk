{
    "name": "zohocrm/php-sdk",
    "description": "Zoho CRM API SDK for PHP",
    "type": "library",
    "keywords": ["zoho", "crm", "api", "sdk"],
    "homepage": "https://github.com/zoho/zcrm-php-sdk",
    "authors": [{
        "name": "Zoho CRM API Team",
        "email": "support@zohocrm.com",
        "homepage": "https://www.zoho.com/crm/help/customer-support.html",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6|^7.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0|^7.0|^8.0"
    },
    "suggest": {
        "ext-fileinfo": "In order to get detailed from uploaded files."
    },
    "autoload": {
        "psr-4": {
            "zcrmsdk\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
